package exercici1;

abstract class Instrument {
	String nom = "";
	double preu = 0.0;
	
	abstract void tocar();
}

class Vent extends Instrument {
	void tocar() {
		System.out.println("Est√† sonant un instrument de vent");
	}
}

public class Main {
	public static void main(String[] args) {
		System.out.println("Hello world!");
	}
}