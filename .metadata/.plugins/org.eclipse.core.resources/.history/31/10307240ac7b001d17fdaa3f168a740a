package n1exercici2;

public class Cotxe {

	private static final MARCA = "Hispano-Suiza";
	private static model;
	private final potencia;
	
	public Cotxe() {
		this.potencia = 1020;
	}
	
	public void accelerar() {
		System.out.println("El cotxe està accelerant.");
	}
	
	public void frenar() {
		System.out.println("El cotxe està frenant.");
	}
}
