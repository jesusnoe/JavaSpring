package exercici1;

abstract class Instrument {
	static {
		System.out.println("Carrega Instrument");
	}
	
	// Atributs
	private String nom = "";
	private double preu = 0.00;
	
	abstract void tocar() ;
	
	// Constructor
	public Instrument() {}
	public Instrument(String nom, double preu) {
		this.nom = nom;
		this.preu = preu;
	}

}
